MemberCache: {
  shape: sql_table

  UserId: character varying(32) {constraint: primary_key}
  Username: character varying(32)
  AvatarUrl: text
}

Karma: {
  shape: sql_table

  MemberId: character varying(32) {constraint: primary_key}
  KarmaValue: integer
  Positive: integer
  Negative: integer
}
